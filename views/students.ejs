<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students Log</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="/styles/students.css"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
    <h1>Students List</h1>
    <table class="table">
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Age</th>
            <th>College</th>
            <th>Batch</th>
            <th></th>
        </tr>
        <% for(var i = 0; i < students.length; i++) {%>
            <tr class="quote">
              <td><a href="/students/<%= students[i].id %>"><%= students[i].firstname %></a></td>
              <td><%= students[i].lastname %></td>
              <td><%= students[i].age %></td>
              <td><%= students[i].college %></td>
              <td><%= students[i].batch %></td>
              <th><a href="/students/edit/<%= students[i].id %>"><i class="fa fa-edit"></i></a></th>
            </tr>
        <% } %>
    </table>
    <a href="/" class="btn btn-primary">Enter New Student</a>
</body>
</html>